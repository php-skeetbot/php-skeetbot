{
	"$schema": "https://getcomposer.org/schema.json",
	"name": "php-skeetbot/php-skeetbot",
	"description": "A boilerplate library for Bluesky bots.",
	"license": "MIT",
	"type": "library",
	"keywords": ["bluesky", "bot", "skeet", "automation"],
	"authors": [
		{
			"name": "smiley",
			"email": "smiley@chillerlan.net",
			"homepage": "https://github.com/codemasher"
		},
		{
			"name": "Contributors",
			"homepage":"https://github.com/php-skeetbot/php-skeetbot/graphs/contributors"
		}
	],
	"funding": [
		{
			"type": "Ko-Fi",
			"url": "https://ko-fi.com/codemasher"
		}
	],
	"homepage": "https://github.com/php-skeetbot/php-skeetbot",
	"support": {
		"issues": "https://github.com/php-skeetbot/php-skeetbot/issues",
		"source": "https://github.com/php-skeetbot/php-skeetbot"
	},
	"minimum-stability": "stable",
	"prefer-stable": true,
	"require": {
		"php": "^8.2",
		"chillerlan/php-dotenv": "^3.0.1",
		"chillerlan/php-httpinterface": "^6.0",
		"chillerlan/php-oauth": "dev-main#86ecd06f4456a7899bbb18275241c4e0e380e8ee",
		"chillerlan/php-settings-container": "^3.2.1",
		"chillerlan/php-standard-utilities": "dev-main#5a2eef90ac144e54f750d2dd4bfa8e30dae6deba",
		"monolog/monolog": "^3.7"
	},
	"require-dev": {
		"phpunit/phpunit": "^11.4",
		"phpmd/phpmd": "^2.15",
		"phpstan/phpstan": "^1.12",
		"phpstan/phpstan-deprecation-rules": "^1.2",
		"squizlabs/php_codesniffer": "^3.10"
	},
	"suggest": {
	},
	"autoload": {
		"psr-4": {
			"PHPSkeetBot\\PHPSkeetBot\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"PHPSkeetBot\\PHPSkeetBotTest\\": "tests"
		}
	},
	"scripts": {
		"phpcs": "@php vendor/bin/phpcs",
		"phpstan": "@php vendor/bin/phpstan",
		"phpstan-baseline": "@php vendor/bin/phpstan --generate-baseline",
		"phpunit": "@php vendor/bin/phpunit"
	},
	"config": {
		"lock": false,
		"sort-packages": true,
		"platform-check": true
	}
}
